#include <windows.h>

#pragma code_page(65001)

1 ICON DISCARDABLE "${WIN32_RUN_ICON_FILEPATH_WIN}"

VS_VERSION_INFO VERSIONINFO
FILEVERSION     ${WIN32_RUN_PROJECT_VERSION}
PRODUCTVERSION  ${WIN32_RUN_PROJECT_VERSION}
FILEOS         	VOS__WINDOWS32
FILETYPE       	VFT_APP
FILESUBTYPE    	VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName",        "${ORGANIZATION_NAME}\0"
            VALUE "FileDescription",    "${WIN32_RUN_APPLICATION_DISPLAY_NAME}\0"
            VALUE "FileVersion",        "${WIN32_RUN_PROJECT_VERSION_STR}\0"
            VALUE "InternalName",       "${WIN32_RUN_PROJECT_INTERNAL_NAME}\0"
            VALUE "LegalCopyright",     "${LEGAL_COPYRIGHT}\0"
            VALUE "LegalTrademarks",    "${LEGAL_TRADEMARKS}\0"
            VALUE "OriginalFilename",   "${WIN32_RUN_ORIGINAL_FILE_NAME}\0"
            VALUE "ProductName",        "${WIN32_RUN_APPLICATION_DISPLAY_NAME}\0"
            VALUE "ProductVersion",     "${WIN32_RUN_PROJECT_VERSION_STR}\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
